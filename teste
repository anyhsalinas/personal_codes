<!-- simple slider docs -->
<link rel="stylesheet" href="https://anyhsalinas.github.io/personal_content/free-simple-slider.css" type="text/css" media="all" />
<script src="https://anyhsalinas.github.io/personal_content/free-simple-slider.js"></script>
<!-- simple slider docs -->

<!-- simple slider -->
<script>
$(document).ready(function () {
/** slider script **/
function sliderSlides() {
    $('#simple-slider').freeSimpleSlider({
        dots: true,  /** bolinhas embaixo do slider **/
        arrows: true,   /** botões de "anterior" e "próximo" **/
        time: 3000,   /** tempo da transição **/
        animation: "{select:Slider Animation}",   /** tipo de animação **/
        prevButton: "<i class='lnr lnr-chevron-left'></i>",   /** botão da esquerda **/
        nextButton: "<i class='lnr lnr-chevron-right'></i>"   /** botão da direita **/
    });
    
    $('#simple-slider-2').freeSimpleSlider({
        dots: true,  /** bolinhas embaixo do slider **/
        arrows: true,   /** botões de "anterior" e "próximo" **/
        time: 3000,   /** tempo da transição **/
        animation: "{select:Slider Animation}",   /** tipo de animação **/
        prevButton: "<i class='lnr lnr-chevron-left'></i>",   /** botão da esquerda **/
        nextButton: "<i class='lnr lnr-chevron-right'></i>"   /** botão da direita **/
    });
};
    
    if($('#simple-slider').children('li').length > 1){
        sliderSlides();
    } else {
        $('#simple-slider').css({"display":"none"});
    };
    
    if($('#simple-slider-2').children('li').length > 1){
        sliderSlides();
    } else {
        $('#simple-slider-2').css({"display":"none"});
    };
});
</script>
<!-- simple slider -->




<!--
        Un-nest tumblr captions plugin
        by neothm.com & magnusthemes.tumblr.com
-->
<script src="http://static.tumblr.com/wgg6svp/I8Yobwo15/unnest.min.js"></script>
<script>
$(document).ready(function() {
    
$(function(){ $('.caixa').unnest({ 
    yourCaption: ".caption", //your caption selector, the div wrapping {Caption}
    wrapName: ".tumblr_parent", //the name of the captions' new wrapper
    newCaptionUsername: true, //if the user adds a new caption, following a series of captions, show their username above the caption
    originalPostCaptionUsername: true, //for the original captions, or a user-added caption that previously didn't have any, have their username on it
    tumblrAvatars: true, //"dashboard style" caption, have the avatar of the blog next to the username
    tumblrAvatarClass: ".tumblr_avatar", //class of the avatar next to the username
    usernameColon: false //if turned to false, removes the colon at the end of usernames
}); });

});
</script>
<!-- Un-nest tumblr captions plugin ends -->



<!-- scripts -->
<script>
$(document).ready(function() {

    
    /** fade in **/
    $("#all").animate({opacity: 1}, 1000);
    /** fade in **/
    
    
    /** slider position **/
    if("{select:Slider Position}" == "Left") {
        $("#left_header").css({"display":"block"});
        $("#right_header").css({"display":"none"});
    } else {
        $("#left_header").css({"display":"none"});
        $("#right_header").css({"display":"block"});
    }
    /** slider position **/
    
    
    /** sticky follow buttons **/
    $(window).scroll(function(){
        if($(window).scrollTop() >= 10) {
            $(".tmblr-iframe").addClass("move");
        } else {
            $(".tmblr-iframe").removeClass("move");
        }
    });
    /** sticky follow buttons **/
    
    
    /** scroll to posts **/
    var loc_hash = location.hash;
    var loc_hash_boo = Boolean(loc_hash);
    
    if (location.pathname != "/" && loc_hash_boo == false) {
     var cont = $("#content").offset().top;
        var cont = cont - 100;
        var cont = String(cont);
    
        $("html, body").animate({
        scrollTop: cont + 'px'
        }, 1000);
    };
    /** scroll to posts **/
    
    /** scroll to hash **/
    if (loc_hash_boo == true) {
        
        var cont2 = $(loc_hash).offset().top;
        var cont2 = cont2 - 85;
        var cont2 = String(cont2);
    
        $("html, body").animate({
        scrollTop: cont2 + 'px'
        }, 1000);
    };

    
    $("a").click(function(){
        var parte = $(this).attr("href");
        
        var cont3 = $(parte).offset().top;
        var cont3 = cont3 - 85;
        var cont3 = String(cont3);
    
        $("html, body").animate({
        scrollTop: cont3 + 'px'
        }, 1000);
        return false;
    });
    /** scroll to hash **/
    


    
/** search **/
function search_for_word() {
    var q_keyword = $("#search_word input:first-child").val();
    location.assign("http://{Name}.tumblr.com/search/" + q_keyword);
};
function search_for_tag() {
    var q_tag = $("#search_tag input:first-child").val();
    location.assign("http://{Name}.tumblr.com/tagged/" + q_tag);
};

$("#search_word").submit(function(){
    search_for_word();
});

$("#search_word_button").click(function(){
    search_for_word();
    return false;
});

$("#search_tag").submit(function(){
    search_for_tag();
});
$("#search_tag_button").click(function(){
    search_for_tag();
    return false;
});
/** search **/
    
    
    /** disappear when empty **/
    /** navigation **/
    var n1 = "{text:Navigate Link1 Title}";
    var n2 = "{text:Navigate Link2 Title}";
    var n3 = "{text:Navigate Link3 Title}";
    var n4 = "{text:Navigate Link4 Title}";
    var n5 = "{text:Navigate Link5 Title}";
    var n6 = "{text:Navigate Link6 Title}";
    var n7 = "{text:Navigate Link7 Title}";
    var n8 = "{text:Navigate Link8 Title}";
    var n9 = "{text:Navigate Link9 Title}";
    var n10 = "{text:Navigate Link10 Title}";
    
    if (n1 + n2 + n3 + n4 + n5 + n6 + n7 + n8 + n9 + n10 == "") {
        $("div#navigate").hide();
    };
    
    
    /** moderators **/
    var m1 = "{text:Member1 Name}";
    var m2 = "{text:Member2 Name}";
    var m3 = "{text:Member3 Name}";
    var m4 = "{text:Member4 Name}";
    var m5 = "{text:Member5 Name}";
    var m6 = "{text:Member6 Name}";
    var m7 = "{text:Member7 Name}";
    var m8 = "{text:Member8 Name}";
    var m9 = "{text:Member9 Name}";
    var m10 = "{text:Member10 Name}";
    
    if (m1 + m2 + m3 + m4 + m5 + m6 + m7 + m8 + m9 + m10 == "") {
        $("div#members").hide();
    };
    
    
    /** updates **/
    var proj1 = "{text:Update1 Title}";
    var proj2 = "{text:Update2 Title}";
    var proj3 = "{text:Update3 Title}";
    var proj4 = "{text:Update4 Title}";
    var proj5 = "{text:Update5 Title}";
    
    if (proj1 + proj2 + proj3 + proj4 + proj5 == "") {
        $("div#updates_box").hide();
    };
    
    
    
    /** partnership **/
    var p1 = "{text:Affiliate1 Name}";
    var p2 = "{text:Affiliate2 Name}";
    var p3 = "{text:Affiliate3 Name}";
    var p4 = "{text:Affiliate4 Name}";
    var p5 = "{text:Affiliate5 Name}";
    var p6 = "{text:Affiliate6 Name}";
    var p7 = "{text:Affiliate7 Name}";
    var p8 = "{text:Affiliate8 Name}";
    var p9 = "{text:Affiliate9 Name}";
    var p10 = "{text:Affiliate10 Name}";
    
    if (p1 + p2 + p3 + p4 + p5 + p6 + p7 + p8 + p9 + p10 == "") {
        $("div#affiliates_box").hide();
    };
    

    /** disappear when empty **/
    
});
</script>
